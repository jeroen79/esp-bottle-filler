CONFIG_COMPILER_CXX_EXCEPTIONS=n

CONFIG_PARTITION_TABLE_CUSTOM=y
CONFIG_PARTITION_TABLE_CUSTOM_FILENAME="partitions.csv"
CONFIG_PARTITION_TABLE_FILENAME="partitions.csv"
CONFIG_PARTITION_TABLE_MD5=y
CONFIG_ESPTOOLPY_FLASHSIZE_4MB=y
CONFIG_ESPTOOLPY_HEADER_FLASHSIZE_UPDATE=y

# CONFIG_ESP_WIFI_ENABLE_WPA3_SAE=n
# CONFIG_ESP_WIFI_ENABLE_WPA3_OWE_STA=n
CONFIG_ESP_WIFI_ENTERPRISE_SUPPORT=n

CONFIG_PM_ENABLE=n

CONFIG_OPTIMIZATION_ASSERTIONS_SILENT=y
CONFIG_COMPILER_OPTIMIZATION_ASSERTION_LEVEL=1
CONFIG_LOG_BOOTLOADER_LEVEL=1
CONFIG_LOG_BOOTLOADER_LEVEL_ERROR=y
# CONFIG_LOG_DEFAULT_LEVEL_ERROR=y
# CONFIG_LOG_DEFAULT_LEVEL=1
# CONFIG_LOG_MAXIMUM_EQUALS_DEFAULT=y
# CONFIG_LOG_MAXIMUM_LEVEL=1
CONFIG_NEWLIB_NANO_FORMAT=y
# CONFIG_ETH_USE_ESP32_EMAC=n
# CONFIG_ETH_USE_SPI_ETHERNET=n

#
# Wifi, some boards seem to have issues at 20dbm so we default to 15, can later be change in gui
#
CONFIG_ESP_PHY_MAX_WIFI_TX_POWER=15
CONFIG_ESP_PHY_MAX_TX_POWER=15